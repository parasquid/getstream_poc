<p><%= @apikey %>
<p><%= @appid %>
<p><%= @user_token %>
<p><%= @notification_token %>

<script>
  var client = stream.connect('<%= @apikey %>', null,  '<%= @appid %>');

  var userFeed = client.feed('user', '7', '<%= @user_token %>')
  var userSubscription = userFeed.subscribe(
    () => userFeed.get().then(data => console.log(data)),
    (errors) => console.error({errors})
  );

  var notificationFeed = client.feed('notification', '7', '<%= @notification_token %>')
  var notificationSubscription = notificationFeed.subscribe(
    () => notificationFeed.get().then(data => console.log(data)),
    (errors) => console.error({errors})
  );

</script>
